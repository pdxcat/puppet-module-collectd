# Generated by Puppet
<% unless @fqdnlookup %>
Hostname <%= scope.lookupvar('::hostname') %>
FQDNLookup false
<% else %>
#Hostname localhost
FQDNLookup true
<% end %>
#BaseDir "/var/lib/collectd"
#PluginDir "/usr/lib/collectd"
<% if @enable_typesdb %>
TypesDB "/usr/share/collectd/types.db"
<% end %>
Interval <%= @interval %>
Timeout <%= @timeout %>
ReadThreads <%= @threads %>
Include "<%= @plugin_conf_dir %>"
