# Generated by Puppet
LoadPlugin snmp

<Plugin snmp>
<% @data.each_key do |key| -%>
  <Data "<%=key %>">
    Type "<%= @data[key]['Type'] %>"
    Table <%= @data[key]['Table'] %>
    Instance "<%= @data[key]['Instance'] %>"
    Values <% @data[key]['Values'].each do |x| -%>"<%= x %>" <% end %>
  </Data>
<% end -%>
<% @hosts.each_key do |key| -%>
  <Host "<%=key %>">
    Address "<%= @hosts[key]['Address'] %>"
    Version <%= @hosts[key]['Version'] %>
    Community "<%= @hosts[key]['Community'] %>"
    Collect <% @hosts[key]['Collect'].each do |x| -%>"<%= x -%>" <% end %>
    Interval <%= @hosts[key]['Interval'] %>
  </Host>
<% end -%>
</Plugin>
