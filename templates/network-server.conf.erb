# Generated by Puppet
<Plugin network>
<% if @collectd_version and (scope.function_versioncmp([@collectd_version, '4.7']) >= 0) -%>
  <Server "<%= @server %>" "<%= @port %>">
    SecurityLevel "<%= @securitylevel %>"
<%- if @username or @password -%>
    Username "<%= @username %>"
    Password "<%= @password %>"
<%- end -%>
<%- if @interface -%>
    Interface "<%= @interface %>"
<%- end -%>
  </Server>
<% else -%>
  Server "<%= @server %>" "<%= @port %>"
<% end -%>
</Plugin>
