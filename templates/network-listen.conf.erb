# Generated by Puppet
<Plugin network>
<% if @collectd_version and (scope.function_versioncmp([@collectd_version, '4.7']) >= 0) -%>
  <Listen "<%= @server %>" "<%= @port %>">
    SecurityLevel "<%= @securitylevel %>"
<%- if @authfile -%>
    AuthFile "<%= @authfile %>"
<%- end -%>
<%- if @interface -%>
    Interface "<%= @interface %>"
<%- end -%>
  </Listen>
<% else -%>
  Listen "<%= @server %>" "<%= @port %>"
<% end -%>
</Plugin>
