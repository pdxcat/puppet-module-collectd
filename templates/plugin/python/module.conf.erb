  Import "<%= @module %>"

  <Module "<%= @module %>">
  <%- @config.sort.each do |key,value| -%>
  <%- if value.is_a?(Array) -%>
  <%- value.sort.each do |v| -%>
    <%= key %> <% if !!v == v %><%= v %><% else %>"<%= Shellwords.split(v).join('" "') %>"<% end %>
  <%- end -%>
  <%- elsif value.is_a?(Hash) -%>
  <%- value.sort.each do |k,v| -%>
    <%= key %> <%= k %> <% if !!v == v %><%= v %><% else %>"<%= Shellwords.split(v).join('" "') %>"<% end %>
  <%- end -%>
  <%- else -%>
  <%- if not !!value == value and value.include?('"') -%>
    <%= key %> "<%= Shellwords.split(value).join('" "') %>"
  <%- else -%>
    <%= key %> <% if !!value == value %><%= value %><% else %>"<%= value %>"<% end %>
  <%- end -%>
  <%- end -%>
  <%- end -%>
  </Module>
